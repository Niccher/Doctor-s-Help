pipeline {
agent none
  stages {
    stage("Cloning") {
    agent any
      steps {
        sh 'echo first run'
      }
    }
  stage("SonarQube Scanner") {
      agent any
        steps {
        withSonarQubeEnv('SonarQ_installation') {
          sh 'echo second run'
        }
        }
      }
node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def scannerHome = tool 'SonarScanner';
    withSonarQubeEnv() {
      sh "${scannerHome}/bin/sonar-scanner"
    }
  }
}
  }
  
}
