pipeline {
agent none
  stages {
    stage("Cloning") {
    agent any
      steps {
        sh 'Echo first run'
      }
    }
  }
stages {
    stage("build & SonarQube Scanner") {
    agent any
      steps {
      withSonarQubeEnv('SonarQ_installation') {
        sh 'Echo second run'
      }
      }
    }
  }
}
